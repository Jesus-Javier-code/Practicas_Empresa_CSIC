        "    latitud = st.number_input(\"Latitud\", value=0.0, format=\"%.6f\")\n",
        "    longitud = st.number_input(\"Longitud\", value=0.0, format=\"%.6f\")\n",
        "    radio = st.number_input(\"Radio de la regi√≥n (km)\", value=100.0, format=\"%.1f\")\n",
        "    \n",
        "    # Bot√≥n para enviar el formulario\n",
        "    submit_button = st.form_submit_button(\"Actualizar displays\")\n",
        "\n",
        "# Ejecutar l√≥gica cuando se presiona el bot√≥n\n",
        "if submit_button:\n",
        "    st.write(\"Procesando datos con los siguientes par√°metros:\")\n",
        "    st.write(f\"Fecha inicial: {fecha_inicial}\")\n",
        "    st.write(f\"Fecha final: {fecha_final}\")\n",
        "    st.write(f\"Latitud: {latitud}\")\n",
        "    st.write(f\"Longitud: {longitud}\")\n",
        "    st.write(f\"Radio: {radio} km\")\n",
        "    # Llamar a tu funci√≥n de procesamiento\n",
        "    try:\n",
        "        # Aqu√≠ puedes llamar a tu funci√≥n de Python\n",
        "        subprocess.run([\"python\", \"../A01_source/B01_2_eq_download/download.py\", \n",
        "                        str(fecha_inicial), str(fecha_final), str(latitud), str(longitud), str(radio)], check=True)\n",
        "        st.success(\"Datos procesados correctamente. Actualizando displays...\")\n",
        "    except Exception as e:\n",
        "        st.error(f\"Error al procesar los datos: {e}\")\n",
        "\n",
        "# Displays actualizados\n",
        "st.write(\"### Mapas de actividad s√≠smica\")\n",
        "st.write(\"Mapa actualizado basado en los par√°metros ingresados:\")\n",
        "st.components.v1.iframe(src=\"./B_images/eq_map.html\", width=700, height=500)\n",
        "\n",
        "st.write(\"### Histogramas de actividad s√≠smica\")\n",
        "st.write(\"Histogramas actualizados:\")\n",
        "st.components.v1.iframe(src=\"./B_images/eq_histogram.html\", width=700, height=500)\n"
